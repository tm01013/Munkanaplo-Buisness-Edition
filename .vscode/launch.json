{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ASP.NET Core Launch (web)",
      "targetArchitecture": "x86_64",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}\\bin\\Debug\\net7.0\\Munkanaplo2.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}\\Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "targetArchitecture": "x86_64",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
